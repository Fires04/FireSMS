[gammu]
port = /dev/ttyUSB0
connection = at115200

[smsd]
Service = files
LogFile = syslog
LogFormat = text
PhoneID = modem1
CommTimeout = 5
SendTimeout = 30
MaxRetries = 5
ReceiveFrequency = 5
InboxPath = /var/spool/gammu/inbox/
OutboxPath = /var/spool/gammu/outbox/
SentSMSPath = /var/spool/gammu/sent/
ErrorSMSPath = /var/spool/gammu/error/
DebugLevel=255
